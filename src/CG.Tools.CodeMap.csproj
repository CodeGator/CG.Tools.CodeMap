    <RepositoryType>git</RepositoryType>
    <PackageTags>nuget, dependency graph, code map, references, CG.Tools.CodeMap, CodeGator</PackageTags>
    <ApplicationIcon>Images\App.ico</ApplicationIcon>
    <PackageIcon>packageDefaultIcon-50x50.png</PackageIcon>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netcoreapp3.1\CG.Tools.CodeMap.xml</DocumentationFile>
